<div id="entry">
  <h1> Welcome to Receipt-Tracker </h1>
  <p>Please provide your login information below or create a new account</p>

</div>

<div id="home">
  <form>
    {{!-- username --}}
    <div class="form-group">
      <label for="formGroupExampleInput">Username:</label>
      <input type="text" class="form-control" id="username" placeholder="username input">
    </div>

    {{!-- password --}}
    <div class="form-group">
      <label for="formGroupExampleInput2">Password:</label>
      <input type="text" class="form-control" id="password" placeholder="password input">
    </div>

    {{!-- submit button --}}
    <button type="submit" class="btn btn-primary mb-2">Submit</button>
    <button type="submit" class="btn btn-primary mb-2">Create new account</button>
  </form>
</div>



<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
